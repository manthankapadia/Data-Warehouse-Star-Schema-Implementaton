  <step>
    <name>Table input_App_Profile</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Source DB</connection>
    <sql>    &#xd;&#xa;    SELECT&#xd;&#xa;      &#x2f;&#x2a; fields that will be joined to dimensions in your surrogate key piplelining &#x2a;&#x2f;&#xd;&#xa;     p.program_code, p.applicant_client_id, p.printed_date, p.submitted_date, p.last_accessed_date&#xd;&#xa;      &#x2f;&#x2a; measures &#x2a;&#x2f;&#xd;&#xa;      &#xd;&#xa;     ,IFNULL&#x28;m.cnt_msg_app,&#x27;NA&#x27;&#x29; AS cnt_msg_app&#xd;&#xa;     , IFNULL&#x28;m.cnt_msg_sch, &#x27;NA&#x27;&#x29; AS cnt_msg_sch&#xd;&#xa;     , 1 AS is_applied&#xd;&#xa;     , CASE d.admission_decision WHEN &#x27;ADMITTED&#x27; THEN 1 &#xd;&#xa;                                 WHEN &#x27;WITHRAW&#x27;  THEN 1&#xd;&#xa;								WHEN &#x27;MATRICULATED&#x27; THEN 1&#xd;&#xa;                                 ELSE 0 &#xd;&#xa;       END AS is_admitted&#xd;&#xa;     , CASE d.admission_decision WHEN &#x27;MATRICULATED&#x27; THEN 1 &#xd;&#xa;                                 ELSE 0 &#xd;&#xa;       END AS is_accepted&#xd;&#xa;  FROM src_app_profile p&#xd;&#xa;  LEFT OUTER JOIN &#x28;&#xd;&#xa;        SELECT applicant_client_id, &#xd;&#xa;	 SUM&#x28;IF&#x28;SENDER_TYPE&#x3d;&#x27;APP&#x27;, 1, 0&#x29;&#x29;AS cnt_msg_app,  &#xd;&#xa;         SUM&#x28;IF&#x28;SENDER_TYPE&#x3d;&#x27;SCH&#x27;, 1, 0&#x29;&#x29; AS cnt_msg_sch &#xd;&#xa;         FROM src_messages m&#xd;&#xa;         GROUP BY applicant_client_id&#xd;&#xa;       &#x29; m&#xd;&#xa;    ON p.applicant_client_id &#x3d; m.applicant_client_id&#xd;&#xa;  LEFT OUTER JOIN src_decision d&#xd;&#xa;    ON p.applicant_client_id &#x3d; d.applicant_client_id&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
     <cluster_schema/>
